  "license": "ISC",
  "dependencies": {
    "@maxhub/max-bot-api": "^0.2.1",
    "@types/node": "^24.7.2",
    "dotenv": "^17.2.3",
    "pino": "^10.0.0",
    "pino-pretty": "^13.1.2",
    "pm2": "^6.0.13",
    "typescript": "^5.9.3",
    "undici": "^7.16.0"
  }
}
