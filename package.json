{
  "name": "max-bot-murman-koncult",
  "version": "1.0.0",
  "description": "Max bot for information Murman Personal Center",
  "main": "bot.js",
  "scripts": {
    "build": "tsc",
    "start": "pm2 start ecosystem.config.js --env production",
    "stop": "pm2 stop max-bot-murman-koncult",
    "restart": "pm2 restart max-bot-murman-koncult",
    "delete": "pm2 delete max-bot-murman-koncult",
    "logs": "pm2 logs max-bot-murman-koncult",
    "status": "pm2 status",
    "monit": "pm2 monit",
    "dev": "tsc && node bot.js",
    "clean": "rimraf *.js *.d.ts *.js.map *.d.ts.map",
    "pm2:start": "npm run build && pm2 start ecosystem.config.js",
    "pm2:reload": "npm run build && pm2 reload ecosystem.config.js"
  },
  "keywords": [],
